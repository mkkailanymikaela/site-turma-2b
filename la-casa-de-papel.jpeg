<figure itemprop="associatedMedia image" itemscope="" itemtype="http://schema.org/ImageObject">
<meta content="https://p2.trrsf.com/image/fget/cf/940/0/images.terra.com/2021/12/09/image002.jpg" itemprop="url"/>
